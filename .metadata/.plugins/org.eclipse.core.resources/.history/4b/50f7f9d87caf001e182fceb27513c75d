package assignment;

class Complex
{
	int real, img;
	public Complex(int real, int img) {
		this.real = real;
		this.img = img;
	}
	
	public void print()
	{
		System.out.println(real+" +i"+img);
	}
}

public class A28 {
	public static void main(String[] args) {
		
		
		Complex c1 = new Complex(10, 20);
		Complex c2 = new Complex(1, 5);
		
		c1.print();
		c2.print();
		
	}
}
