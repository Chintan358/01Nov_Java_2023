package oops;

class Student
{
	int id;
	String name;
	
	Student()
	{
		System.out.println("Constructor calling..");
	}
	
	Student(int i)
	{
		System.out.println("value : "+i);
	}
	
	public void display()
	{
		System.out.println(id+" "+name);
	}
}

public class O002_Constructor {
	public static void main(String[] args) {
		
		Student st = new Student(10);
		st.display();
		
	}
}
