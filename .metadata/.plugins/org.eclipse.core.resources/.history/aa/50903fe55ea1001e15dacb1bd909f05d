package collection;

import java.util.ArrayList;

public class C002_ArrayList {
	public static void main(String[] args) {
		
		
		ArrayList<String> sub = new ArrayList<String>();
		sub.add("Java");
		sub.add("python");
		sub.add("android");
		
		
		ArrayList<String> sports = new ArrayList<String>();
		sports.add("cricket");
		sports.add("Hockey");
		
		
		
		
	}
}
