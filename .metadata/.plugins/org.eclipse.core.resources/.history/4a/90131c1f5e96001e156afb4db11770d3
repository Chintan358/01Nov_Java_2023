package oops;
class P
{
	P()
	{
		System.out.println("Running P class cons....");
	}
}

class Q
{
	Q()
	{
		System.out.println("Running Q class cons....");
	}
}


class R
{
	R()
	{
		System.out.println("Running R class cons....");
	}
}

public class O008_ConstructorChaining {
	public static void main(String[] args) {
		
		R r = new R();
		
	}
}
