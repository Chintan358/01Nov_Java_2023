package fileio;

import java.io.File;

public class F004_file {
	public static void main(String[] args) {
		
		
		File file = new File("C:\\Chintan_work\\Files\\home.txt");
		
		
		if(file.exists())
		{
			System.out.println("exist");
		}
		else
		{
			System.out.println("not found...");
		}
		
		
	}
}
