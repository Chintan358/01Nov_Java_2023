<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:context="http://www.springframework.org/schema/context"
  xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">
  
  	<!-- autowire = "byName" -->
  	
  	<!-- autowire = "byType" -->
  	<bean class="com.User" id="user" autowire="byType">
  	<property name="id" value="10"></property>
  	<property name="name" value="Mayank"></property>
  	<property name="email" value="mayank@gmail.com"></property>
  	<property name="phone" value="9585748596"></property>
  	<property name="lang" >
  	<list>
  		<value>Gujarati</value>
  		<value>Hindi</value>
  		<value>English</value>
  		<value>English</value>
  	</list>
  	</property>
  	<property name="sub">
  	<set>
	  	<value>Java</value>
	  	<value>Php</value>
	  	<value>Android</value>
  	</set>
  	</property>
  	<!-- <property name="adr" ref="adr"></property> -->
  	<!-- <property name="info">
  	<list>
  	<ref bean="info1"/>
  	<ref bean="info2"/>
  	</list>
  	</property>-->
  	</bean> 
  
  
  	<bean class="com.Address" id="adr1">
  	<property name="city" value="Surat"></property>
  	<property name="state" value="Gujarat"></property>
  	<property name="country" value="India"></property>
  	</bean>
  
  	<bean class="com.Info" id="info1">
  	<property name="phone1" value="8574859685"></property>
  	</bean>
  	
  	<bean class="com.Info" id="info2">
  	<property name="phone1" value="6352415263"></property>
  	</bean>

</beans>